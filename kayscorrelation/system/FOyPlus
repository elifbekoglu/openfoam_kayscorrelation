yPlus1
{
    // Mandatory entries
    type            yPlus;
    libs            ("libfieldFunctionObjects.so");

    // Optional (inherited) entries
    writePrecision  10;
    writeToFile     true;
    useUserTime     true;

    region          region0;
    enabled         true;
    log             true;
    timeStart       0;
    timeEnd         100000;
    executeControl  timeStep;
    executeInterval 1;
    writeControl    timeStep;
    writeInterval   500;
}


